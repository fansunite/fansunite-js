{
  "name": "fansunite-js",
  "version": "0.0.1",
  "description": "JavaScript library for interacting with FansUnite Smart Contracts",
  "main": "./lib/src/index.js",
  "typings": "./lib/src/index.d.ts",
  "author": "Fansunite Entertainment Inc",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/fansunite/fansunite-js.git"
  },
  "scripts": {
    "build": "tsc && copyfiles -u 2 './src/artifacts/*.json' ./lib/src/artifacts",
    "clean": "shx rm -rf lib",
    "test": "run-s clean build run_mocha",
    "lint": "tslint --project . 'src/**/*.ts' 'test/**/*.ts'",
    "run_mocha": "mocha lib/test/*.js",
    "prettier": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "postinstall": "rm -f node_modules/web3/index.d.ts",
    "rebuild_artifacts": "scripts/build_artifacts.sh",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/underscore": "^1.8.9",
    "@types/web3": "^1.0.3",
    "chai": "^4.1.2",
    "copyfiles": "^2.0.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "1.14.2",
    "shx": "^0.3.2",
    "truffle": "^4.1.14",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "bignumber.js": "^7.2.1",
    "fansunite-core": "https://github.com/fansunite/fansunite-core.git#bet-manager/partial-fill",
    "web3": "^1.0.0-beta.35"
  }
}
